{
  "manifest_version": 2,
  "name": "S Marks",
  "description": "A set of utilities for bookmark syncing, editing and display  ",
  "version": "0.1.7",
  "permissions": [
    "bookmarks",
    "storage",
    "menus",
    "menus.overrideContext"
  ],
  "optional_permissions": [
    "privacy"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "smarks@apps.anasta.si",
      "strict_min_version": "75.0",
      "update_url": "https://smarks.apps.anasta.si/updates.json"
    }
  }  
}